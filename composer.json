{
    "name": "delarocha/php-blms-sdk",
    "description": "BLMS SDK for Laravel/Lumen",
    "keywords": ["blms", "sdk"],
    "type": "library",
    "homepage": "https://github.com/dlr-delarocha/php-blms-sdk.git",
    "require": {
        "php":  ">=7.1",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/support": "^6.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Allows for implementation of the Guzzle HTTP client"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alberto de la Rocha",
            "email": "delarocha.alberto01@gmail.com"
        },
        {
            "name": "SRAX",
            "homepage": "https://github.com/dlr-delarocha/php-blms-sdk/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "BLMS\\": "src/BLMS/"
        },
        "files": ["tests/BLMSTestCredencials.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "BLMS\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BLMS\\BLMSServiceProvider"
            ]
        }
    }
}
